function LongDeclarante ( pTipoDoc, pDocumen, pNombre, pDireccion )
{	var LongDeclarante = true;
	if (!( pTipoDoc.length == 0 || Trim(pTipoDoc)  == ' ' ))
	{	if ( Trim(pNombre).length < 4 )
		{	alert("LONGITUD DEL NOMBRE NO VALIDA DEBE SER MAYOR A 4");
			LongDeclarante = false;
		}
		if ( (pTipoDoc == "4") && (!(Trim(pDocumen).length == 11)) )
		{	alert("LONGITUD DE RUC INVALIDO DEBE SER DE 11 CARACTERES");
			LongDeclarante = false;
		}
		if ( (pTipoDoc == "2" || pTipoDoc == "3") && (!(Trim(pDocumen).length == 8)) )
		{	alert("LONGITUD DE LE/DNI INVALIDO DEBE SER DE 8 CARACTERES");
			LongDeclarante = false;
		}
		if ( (pTipoDoc == "6" || pTipoDoc == "7") && ( (Trim(pDocumen).length < 4) || (Trim(pDocumen).length > 11) ) )
		{	alert("LONGITUD DE PASAPORTE/CARNET DE EXTRANJERIA INVALIDO DEBE SER ENTRE 4 Y 11 CARACTERES");
			LongDeclarante = false;
		}
		if ( (pTipoDoc == "10" || pTipoDoc == "12") )
		{	if ( Trim(pDocumen).length > 10 )
			{	alert("LONGITUD DE LIBRETA MILITAR/LICENCIA DE CONDUCIR INVALIDO DEBE SER DE A LO MAS 10 CARACTERES");
				LongDeclarante = false;
			}
		}
		else // Demás tipos de Documento (Que se insertan al DECLARAN)  
		{	if ( Trim(pDireccion).length > 0 && Trim(pDireccion).length < 6 )
			{	alert("LONGITUD DE DIRECCION DEBE SER DE POR LO MENOS 6 CARACTERES");
				LongDeclarante = false;
			}
		}
	}
	return LongDeclarante;
}

function ValidacionHora(hora) 
{   if ( (hora.length !=5) || 
		(hora.substring(2,3) !=':') || 
		(hora.substring(0,2) > 24) || 
		(hora.substring(3,5) > 59))
	{	alert("Ingrese la hora correctamente (5 caracteres incluyendo :)");
        return false;
    }
	return true;
}

