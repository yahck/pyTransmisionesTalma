function StartYear() {
TDate = new Date();
CurYear = TDate.getYear();
	if (CurYear < 1980) 
	{
		if(CurYear < 100)
		{
			CurYear += 2000;
		} else {
			CurYear += 1900;
		}
	}
return  CurYear;;
}


