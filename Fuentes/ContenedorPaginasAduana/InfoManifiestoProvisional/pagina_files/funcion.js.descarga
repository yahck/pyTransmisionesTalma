      if ( !Esnumero(num) )  { 
           alert('Ingrese correctamente el RUC'); 
           return false; 
      } 
      else if ( num.substring(0,2) != '10' && num.substring(0,2) != '20'  )  { 
           alert('Ingrese correctamente el RUC'); 
           return false; 
      } 
     else if (  num.length != 11 )  {    
           alert('Verifique el RUC debe contener 11 digitos');  
           return false;  
     } 
     else {    
           a = num.substring(0, 1)*5 + num.substring(1, 2)*4 + num.substring(2, 3)*3 + num.substring(3, 4)*2 + num.substring(4, 5)*7 + num.substring(5, 6)*6 + num.substring(6, 7)*5 + num.substring(7, 8)*4 + num.substring(8, 9)*3 + num.substring(9, 10)*2 ;  
           b = a/11;  
           b =  Math.floor( b);  
           c = 11 - ( a - b*11  );  
           a = num.substring(10, 11) 
           if ( c == 10 )  
               c = 0;  
           else if ( c == 11 )  
               c = 1;  
                 
           if ( a != c ) { 
              alert('Verifique  RUC es incorrecto');  
              return false;  
            }   
  
    } 
    return true;  
 }

